{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}"
        },

        {
            "name": "Python: debug with args",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/run.py",
            
            "env": {
                "CUDA_VISIBLE_DEVICES": "0"
            },
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/src"  
        },

        {
            "name": "Python: train TimesNet Long-Term Forecast (Learning)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/run.py",
            "console": "integratedTerminal",
            "env": {
                "CUDA_VISIBLE_DEVICES": "0"
            },
            "args": [
                "--task_name", "long_term_forecast",
                "--is_training", "1",
                "--root_path", "./dataset/ETT-small/",
                "--data_path", "ETTh1.csv",
                "--model_id", "ETTh1_96_32",
                "--model", "TimesNet",
                "--data", "ETTh1",
                "--features", "S",
                "--seq_len", "96",
                "--label_len", "48",
                "--pred_len", "32",
                "--e_layers", "2",
                "--d_layers", "1",
                "--factor", "3",
                "--enc_in", "1",  // number of features in raw data, 这个参数定义的是模型的 DataEmbedding  的输入 attribute 数量
                "--dec_in", "7",  // 这个 dec_in 是啥？
                "--c_out", "5",
                "--d_model", "16",
                "--d_ff", "32",
                "--des", "Exp",
                "--itr", "1",
                "--top_k", "5"
            ]
        }
    ]
}